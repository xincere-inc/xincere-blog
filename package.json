{
  "name": "xincere-blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "import": {
      "schemas": "./prisma/schema/models/*.prisma",
      "output": "./prisma/schema.prisma"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack --experimental-https",
    "build": "next build",
    "build:ci": "npx prisma generate && npx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.{ts,js,tsx}\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.{ts,js,tsx,scss}\"",
    "format": "prettier --write .",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "db:generate": "npx prisma-import generate --force",
    "db:migrate": "npm run db:generate && npx prisma migrate dev && npx prisma generate",
    "db:migrate:local": "npm run db:generate && DATABASE_URL='postgresql://postgres:postgres@127.0.0.1:54322/postgres' sh -c 'npx prisma migrate reset --skip-seed && npm run db:migrate && npx prisma db seed'",
    "db:migrate:docker": "npx prisma migrate deploy && npx prisma generate",
    "db:deploy": "npx prisma migrate deploy",
    "db:studio": "DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres npx prisma studio",
    "db:reset": "DATABASE_URL='DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres' sh -c 'npx prisma migrate reset --skip-generate && npx prisma db seed'",
    "generate:client": "openapi-generator-cli generate -g typescript-axios -i ./src/api/specs/swagger.yaml -o ./src/api/client",
    "prepare": "husky",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --write",
    "*.{css,scss}": [
      "stylelint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.2",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@aws-sdk/client-s3": "^3.844.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@prisma/client": "^6.9.0",
    "@supabase/supabase-js": "^2.53.0",
    "@uiw/react-markdown-editor": "^6.1.4",
    "antd": "^5.25.4",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "date-fns": "^4.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.513.0",
    "marked": "^16.0.0",
    "next": "15.3.3",
    "next-auth": "^5.0.0-beta.28",
    "nodemailer": "^6.6.5",
    "puppeteer": "^23.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-paginate": "^8.3.0",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui": "^5.24.0",
    "typescript-eslint": "^8.33.1",
    "uuid": "^11.1.0",
    "world-countries": "^5.1.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@faker-js/faker": "^9.8.0",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/react-tagsinput": "^3.20.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "prisma-erd-generator": "^2.0.4",
    "supabase": "^2.33.5",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
