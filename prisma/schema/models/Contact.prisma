model Contact {
  id            Int       @id @default(autoincrement())
  companyName   String
  contactName   String
  email         String
  phone         String
  inquiry       String
  privacyPolicy Boolean
  status        ContactStatus @default(OPEN) @map("status")
  createdAt     DateTime  @default(now()) @map("created_at")

  @@map("contacts")
}